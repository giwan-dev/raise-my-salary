{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-airbnb",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
      "import-name": [
        true,
        {
          "react": "React",
          "reactDom": "ReactDOM"
        }
      ],
      "interface-name": [true, "never-prefix"],
      "ordered-imports": true,
      "quotemark": [true, "single", "jsx-double"],
      "semicolon": [true, "never"],
      "variable-name": [true, "ban-keywords"]
    },
    "rulesDirectory": []
}
